{
	"name": "editor",
	"version": "0.1.0",
	"homepage": "./",
	"main": "public/electron.js",
	"private": true,
	"dependencies": {
		"@material-ui/styles": "^4.3.3",
		"@types/jest": "24.0.18",
		"@types/node": "12.7.2",
		"@types/react": "16.9.2",
		"@types/react-dom": "16.9.0",
		"cross-env": "^5.2.0",
		"electron-is-dev": "^1.1.0",
		"jsbarcode": "^3.11.0",
		"nanoid": "^2.0.4",
		"qrcode": "^1.4.1",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-router-dom": "^5.0.1",
		"react-scripts": "3.1.1",
		"rimraf": "^3.0.0",
		"typescript": "^3.6.2",
		"use-react-router": "^1.0.7"
	},
	"author": {
		"name": "Sunlive"
	},
	"build": {
		"appId": "com.my-website.my-app",
		"productName": "Sticker Generator",
		"copyright": "Copyright Â© 2019 ${author}",
		"mac": {
			"category": "public.app-category.utilities"
		},
		"files": [
			"dist/**/*",
			"node_modules/**/*"
		],
		"directories": {
			"buildResources": "assets"
		}
	},
	"scripts": {
		"watch": "tsc --watch -p ./tsconfig.dev.json",
		"start": "react-scripts start",
		"lint": "prettier --write \"src/**/*{.ts,.tsx,.js}\"",
		"dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
		"app": "electron ./public/electron.js",
		"build": "rescripts build",
		"test": "rescripts test",
		"tsc": "tsc -p ./tsconfig.dev.json",
		"postinstall": "electron-builder install-app-deps",
		"eject": "react-scripts eject",
		"dist": "electron-builder",
		"postdist": "rimraf dist app"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@rescripts/cli": "^0.0.11",
		"@rescripts/rescript-env": "^0.0.10",
		"@types/nanoid": "^2.0.0",
		"@types/qrcode": "^1.3.4",
		"@types/react-router-dom": "^4.3.5",
		"concurrently": "^4.1.2",
		"electron": "^6.0.6",
		"electron-builder": "^21.2.0",
		"prettier": "^1.18.2",
		"wait-on": "^3.3.0"
	}
}
